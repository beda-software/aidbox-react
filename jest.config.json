{
    "transform": {
        ".ts": "ts-jest"
    },
    "testRegex": "__tests__\\/.*\\.(test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": ["./hooks", "./libs", "./services"],
    "verbose": true
}
